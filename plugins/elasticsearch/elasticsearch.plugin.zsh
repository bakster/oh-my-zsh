alias elastic_start='elasticsearch -f -D es.config=/usr/local/Cellar/elasticsearch/0.19.7/config/elasticsearch.yml'
alias elastic_stop='launchctl unload -wF ~/Library/LaunchAgents/homebrew.mxcl.elasticsearch.plist'
alias elastic_log='open /usr/local/var/log/elasticsearch/elasticsearch_bakari.log'
alias elastic_cluster='open /usr/local/var/elasticsearch/elasticsearch_bakari/'
